generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum DoctorSpecialty {
    GENERAL
    PEDIATRIC
    NEUROLOGIST
    CARDIOLOGIST
}

model Doctor {
    id           String            @id
    name         String
    email        String            @unique
    password     String
    registeredAt DateTime
    specialties  DoctorSpecialty[]
}
